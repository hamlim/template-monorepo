{
  "name": "root",
  "workspaces": [
    "docs",
    "packages/*"
  ],
  "private": true,
  "devDependencies": {
    "dprint": "^0.45.0",
    "turbo": "^1.12.4"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build ",
    "build:packages": "turbo run build --filter=./packages/*",
    "type-check": "turbo run type-check",
    "lint": "turbo run lint",
    "format": "dprint fmt",
    "format:check": "dprint check",
    "test": "turbo run test"
  }
}
